# Set default values for all following accounts.
defaults
auth on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

# Gmail
account        gmail
host           smtp.gmail.com
port           465
tls_starttls   off
user           yebenmy@gmail.com
from           yebenmy@gmail.com

# Set a default account
account default: gmail
