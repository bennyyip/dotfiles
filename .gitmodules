[submodule "dotbot"]
	path = dotbot
	url = https://github.com/anishathalye/dotbot
	ignore = dirty
[submodule "vim"]
	path = vim
	url = git@github.com:bennyyip/dot-vim.git
	update = rebase
	ignore = dirty
[submodule "minimal/vim/pack/vendor/start/vim-sensible"]
	path = minimal/vim/pack/vendor/start/vim-sensible
	url = git@github.com:tpope/vim-sensible.git
[submodule "minimal/vim/pack/vendor/start/vim-surround"]
	path = minimal/vim/pack/vendor/start/vim-surround
	url = git@github.com:tpope/vim-surround.git
[submodule "minimal/vim/pack/vendor/start/vim-unimpaired"]
	path = minimal/vim/pack/vendor/start/vim-unimpaired
	url = git@github.com:tpope/vim-unimpaired
[submodule "zsh/.zsh/plugin/fast-syntax-highlighting"]
	path = zsh/.zsh/plugin/fast-syntax-highlighting
	url = https://github.com/zdharma-continuum/fast-syntax-highlighting
